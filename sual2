namespace sual2
{
    internal class Program
    {
        static void Main(string[] args)
        {

            int eded = 0, mertebe = 0;
            Console.Write("ededi  yaz :");
            eded = Convert.ToInt32(Console.ReadLine());
            while (eded > 0)
            {
                eded = eded / 10;
                mertebe++;
            }
            Console.WriteLine("mertebe :Â " + mertebe);




        }
     
    }



